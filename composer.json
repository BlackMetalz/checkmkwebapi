{
    "name": "bheisig/checkmkwebapi",
    "version": "0.2",
    "description": "Easy-to-use, but feature-rich client library for Check_MK's Web API",
    "type": "library",
    "keywords": ["check_mk", "checkmk", "wato", "monitoring", "monitoring love", "api"],
    "homepage": "https://github.com/bheisig/checkmkwebapi",
    "license": "AGPL-3.0+",
    "authors": [
        {
            "name": "Benjamin Heisig",
            "email": "benjamin@heisig.name",
            "homepage": "https://benjamin.heisig.name/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/bheisig/checkmkwebapi/issues",
        "source": "https://github.com/bheisig/checkmkwebapi"
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "sebastian/phpcpd": ">=3.0.1",
        "phpstan/phpstan": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "bheisig\\checkmkwebapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bheisig\\checkmkwebapi\\tests\\": "tests/"
        }
    }
